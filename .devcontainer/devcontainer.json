{
  "name": "Multi-Agent Streamlit Dev",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "streamlit",
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]
    }
  },
  "forwardPorts": [8501],
  "postAttachCommand": {
    "server": "streamlit run app.py --server.enableCORS false --server.enableXsrfProtection false"
  }
}
